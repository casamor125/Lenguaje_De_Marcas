<%- include ("header") %>


  <form action="/editUsuari" method="post" class="container-sm mt-5">

    <input hidden value="<%= usuari.id %>" name="id">
 
    <form action="/editProducte" method="post" class="container-sm mt-5">
      <div class="mb-3">
        <label for="nom" class="form-label">nom</label>
        <input type="text" class="form-control" name="nom" id="nom" required>
        <div  class="form-text">Insert the name of the user</div>
      </div>
      <div class="mb-3">
          <label for="email" class="form-label">email</label>
          <input type="email" class="form-control" name="email" id="email" required>
          <div class="form-text">Insert the email of the user</div>
        </div>
      <button type="submit" class="btn btn-primary ">Submit</button>
    </form>


<%- include("footer")%>